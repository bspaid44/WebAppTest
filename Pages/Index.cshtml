@page
@model IndexModel
@{
    ViewData["Title"] = "Home";
}

<div class="gallery-title">
    <div class="gallery-title-msg">
        <h1>PARALLAX GALLERY</h1>
    </div>
</div>

<div class="main">
    <div id="scene" data-hover-only="true" data-relative-input="true "class="content" data-limit-x="100" data-limit-y="20">
